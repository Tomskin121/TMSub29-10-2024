    <form id="Updateform" class = "  width100  flex flexcolumn rowgap10  flexcentralalign" >
        <h1 id = "textaligncenter width100 height10 block smalllinehight" class = ""> Update Account </h1>
    
        <div class="input-wrapper width80  block">
            <label for="FirstName">First Name</label><br>
            <input  class="overlayinputText height100 width100" type="text" id="FirstName" value="<%= customerdata[0].Customer_FirstName %>"><br>
        </div>
    
        <div class="input-wrapper width80  block">
            <label for="Surname">Surname</label><br>
            <input  class="overlayinputText height100 width100" type="text" id="Surname" value="<%= customerdata[0].Customer_SurName %>"><br>
        </div>
    
        <div class="input-wrapper width80  block">
