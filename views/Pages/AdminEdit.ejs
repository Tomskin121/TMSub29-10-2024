            <input  class="overlayinputText height100 width100" type="text" id="MobilePhone" value="<%= customerdata[0].Customer_MobilePhone %>"><br>
        </div>
    
        <div class="input-wrapper width80  block">
            <label for="Email">E-mail address</label><br>
            <input  class="overlayinputText height100 width100" type="text" id="Email" value="<%= customerdata[0].Customer_Email%>"><br>
        </div>
    
      
    
    
        <input class = "width40 height20 block overlayinputinputbutton position-relative blockcentral" id = "updateaccount" type="Submit" value="Update">
    
    
